				</Canvas.Background>
				<local:MenuItem x:Name="mi1" d:LayoutOverrides="Width, Height" Canvas.Left="0" Canvas.Top="0"/>
			</Canvas>
			<Canvas x:Name="c2" Height="148" Margin="8,209,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="171">
