<Window x:Class="ObjectTableForms.Forms.Debug.PerformanceViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PerformanceViewer" Height="293" Width="284" Closing="Window_Closing">
