        <Label Content="Zeit zwischen DepthFrames:" Height="28" HorizontalAlignment="Left" Margin="12,9,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Dauer Objekterkennung:" Height="28" HorizontalAlignment="Left" Margin="12,43,0,0" Name="label2" VerticalAlignment="Top" />
        <Label Content="Dauer Rotationserkennung:" Height="28" HorizontalAlignment="Left" Margin="12,111,0,0" Name="label3" VerticalAlignment="Top" />
        <Label Content="Dauer pro Frame:" Height="28" HorizontalAlignment="Left" Margin="12,145,0,0" Name="label4" VerticalAlignment="Top" />
        <Label Content="effektive Framerate:" Height="28" HorizontalAlignment="Left" Margin="12,179,0,0" Name="label5" VerticalAlignment="Top" />
        <Label Content="Label" Height="28" HorizontalAlignment="Left" Margin="175,9,0,0" Name="l_depthframes" VerticalAlignment="Top" Background="#FFCEFFFB" Width="75" />
        <Label Background="#FFCEFFFB" Content="Label" Height="28" HorizontalAlignment="Left" Margin="175,43,0,0" Name="l_recognition" VerticalAlignment="Top" Width="75" />
