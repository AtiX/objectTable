        <Label Content="Dauer Tracking:" Height="28" HorizontalAlignment="Left" Margin="12,77,0,0" Name="label6" VerticalAlignment="Top" />
        <Label Background="#FFCEFFFB" Content="Label" Height="28" HorizontalAlignment="Left" Margin="175,77,0,0" Name="l_tracking" VerticalAlignment="Top" Width="75" />
        <Label Content="durchschnittl. Framerate:" Height="28" HorizontalAlignment="Left" Margin="12,211,0,0" Name="label7" VerticalAlignment="Top" />
        <Label Background="#FFCEFFFB" Content="Label" Height="28" HorizontalAlignment="Left" Margin="175,211,0,0" Name="l_averageFps" VerticalAlignment="Top" Width="75" />
    </Grid>
</Window>
